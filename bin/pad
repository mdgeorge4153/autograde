#!/usr/bin/python

import sys

f = sys.stdin

if len(sys.argv) > 1:
	f = open(sys.argv[1], 'r')

input = [l.strip() for l in f.readlines()]
sizes = []

for line in input:
	fields = line.split('\t')
	while len(sizes) < len(fields):
		sizes.append(0)

	for i,f in enumerate(fields):
		sizes[i] = max(sizes[i], len(f))

for line in input:
	for i,f in enumerate(line.split('\t')):
		sys.stdout.write(f.ljust(sizes[i]))
		sys.stdout.write('\t')
	sys.stdout.write('\n')


